# Environments
.env
.env.*
!.env.example
.venv
env/
venv/
ENV/

# Python Caches
__pycache__/
*.py[cod]
*$py.class
.pytest_cache/
.mypy_cache/

# Build and Distribution artifacts
build/
dist/
eggs/
*.egg-info/
*.egg

# Database files
*.sqlite3
*.db

# OS-specific files
.DS_Store
Thumbs.db

# ===================================================================
# Project-Specific Exclusions for running the code
# These directories contain generated data, caches, or user uploads
# and are not needed for a collaborator to run the project.
# ===================================================================

# Top-level directories to ignore completely
/agent_memory.pkl
/generated_documents/
/parser_output/
/rag_storage/
/pdf_embedding_storage/
/uploads/
/templates_storage/

# Ignoring nested directories
/image_rag_system/chroma/
/image_rag_system/uploads/
/image_rag_system/rag_storage/

/juvenile-MCP-client/outputs/
/juvenile-MCP-client/generated_docs/
/juvenile-MCP-client/generated_documents/
/juvenile-MCP-client/uploads/
/juvenile-MCP-client/rag_storage/
/juvenile-MCP-client/templates_storage/
/juvenile-MCP-client/assets/

# This is the most important section for avoiding LFS objects
/Paper2Poster/Paper2Poster/models_cache/
/Paper2Poster/Paper2Poster/assets/
/Paper2Poster/Paper2Poster/*_analysis/
/Paper2Poster/Paper2Poster/*_final/
/Paper2Poster/Paper2Poster/parser_output/

